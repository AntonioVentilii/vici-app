{
	"name": "@caffeine/template-frontend",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"setup": "pnpm i && dfx canister create backend && dfx generate backend && dfx deploy",
		"start": "vite --port 3000",
		"prebuild": "dfx generate backend",
		"build:skip-bindings": "vite build && pnpm copy:env",
		"copy:env": "cp env.json dist/",
		"typescript-check": "svelte-check --tsconfig ./tsconfig.json",
		"format": "prettier --write \"src/**/*.{json,js,ts,svelte,css,scss}\"",
		"lint": "eslint src --ext .ts,.js,.svelte",
		"lint:fix": "eslint src --ext .ts,.js,.svelte --fix"
	},
	"devDependencies": {
		"@eslint/js": "~9.27.0",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/typography": "0.5.10",
		"@tsconfig/svelte": "^5.0.4",
		"@types/node": "^20.9.0",
		"@typescript-eslint/eslint-plugin": "^8.32.1",
		"@typescript-eslint/parser": "^8.32.1",
		"@typescript-eslint/utils": "^8.32.1",
		"autoprefixer": "^10.4.20",
		"dotenv": "^16.5.0",
		"dotenv-cli": "^8.0.0",
		"eslint": "~9.27.0",
		"eslint-plugin-svelte": "^2.43.0",
		"eslint-plugin-tailwindcss": "~3.18.2",
		"globals": "~16.2.0",
		"postcss": "^8.4.41",
		"svelte": "^4.2.19",
		"svelte-check": "^3.8.5",
		"tailwindcss": "^3.4.17",
		"tailwindcss-animate": "^1.0.7",
		"tslib": "^2.6.3",
		"typescript": "^5.8.3",
		"vite": "^5.4.1",
		"vite-plugin-environment": "^1.1.3"
	},
	"dependencies": {
		"@dfinity/agent": "~3.3.0",
		"@dfinity/identity": "~3.3.0",
		"@dfinity/auth-client": "~3.3.0",
		"@dfinity/candid": "~3.3.0",
		"@dfinity/principal": "~3.3.0",
		"@icp-sdk/core": "~4.1.0",
		"lucide-svelte": "^0.435.0",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.1",
		"tailwind-merge": "^2.5.2",
		"date-fns": "^3.6.0",
		"sonner": "^1.7.4",
		"next-themes": "~0.4.6"
	}
}
